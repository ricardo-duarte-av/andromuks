<?xml version="1.0" encoding="utf-8"?><!--
   Backup rules file - excludes sensitive authentication data from cloud backup
   See https://developer.android.com/guide/topics/data/autobackup
   for details.
   Note: This file is ignored for devices older than API 31
   See https://developer.android.com/about/versions/12/backup-restore
-->
<full-backup-content>
    <!-- Exclude authentication and session data from backup for security -->
    <exclude domain="sharedpref" path="AndromuksAppPrefs.xml"/>
    <exclude domain="sharedpref" path="fcm_prefs.xml"/>
    <exclude domain="sharedpref" path="web_client_prefs.xml"/>
    <exclude domain="sharedpref" path="fcm_notification_manager_prefs.xml"/>
</full-backup-content>